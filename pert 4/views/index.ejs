    <% let title = "Template engine" %>
    <%= title %>

    <br>
    <%= caption %>

    <table>
        <tr>
            <th>No</th>
            <th>NPM</th>
            <th>Nama</th>
        </tr>

        <% data.forEach((element,no) => { %>
            <tr>
                <td><%= no+1 %></td>
                <td><%= element.npm %></td>
                <td><%= element.nama  %></td>
            </tr>
        <%}); %>
    </table>
